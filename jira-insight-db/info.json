        {
          "title": "Insight Query Language(IQL)",
          "description": "Specify the query to determine the objects being fetched. E.g. objectType = \"Computer\". The empty IQL means all objects.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "text",
          "tooltip": "objectType = \"Host\" AND Operating System = \"Ubuntu (64-bit)\"",
          "placeholder": "objectType = \"Host\" AND Operating System = \"Ubuntu (64-bit)\"",
          "name": "iql",
          "value": ""
        },
        {
          "title": "Page Number",
          "description": "Specify the page to fetch when paginating through the response from your configured Jira server.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "integer",
          "name": "page",
          "value": 1
        },
        {
          "title": "Result Per Page",
          "description": "Specify the number of objects returned per page from your configured Jira server.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "integer",
          "name": "resultPerPage",
          "value": 25
        },
        {
          "title": "Include Attributes",
          "description": "Specify whether the object attributes be included in the response. If this parameter is false, only the information on the object is returned and the object attributes are not present. By default, it is set to True.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "checkbox",
          "name": "includeAttributes",
          "value": true
        },
        {
          "title": "Include Attributes Deep",
          "description": "Specify the level depth of attributes to be included. For example, consider an object A that has a reference to object B that further has a reference to object C. If object A is included in the response and include attribute depth is set to 1 u.e.includeAttributesDeep=1, object A's reference to object B is included in the attributes of object A; however, object B's reference to object C is not included. To include object B's reference to object C object B's attributes set include attribute depth to 2 i.e. includeAttributesDeep=2.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "integer",
          "name": "includeAttributesDeep",
          "value": 1
        },
        {
          "title": "Include Type Attributes",
          "description": "Select to include the object type attribute definition for each attribute returned with the objects. By default, this parameter is set to False.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "checkbox",
          "name": "includeTypeAttributes",
          "value": false
        },
        {
          "title": "Include Extended Info",
          "description": "Select to include the following information: open issues, if open issues are associated with objects, if objects have attachments. By default set to False. ",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "checkbox",
          "name": "includeExtendedInfo",
          "value": false
        }
      ]
